{
  "version": 2,
  "functions": {
    "api/index.js": {
      "memory": 128,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/api",
      "dest": "/api/index.js"
    }
  ],
  "headers": [
    {
      "source": "/api",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=1800, s-maxage=1800, stale-while-revalidate=86400"
        }
      ]
    }
  ]
}
